440\\u043E\\u0433\\u0443 \\u0432 \\u0431\\u0435\\u0437\\u0433\\u0440\\u0430\\u043D\\u0438\\u0447\\u043D\\u044B\\u0439 \\u043C\\u0438\\u0440 \\u043A\\u043E\\u0434\\u0438\\u043D\\u0433\\u0430. \\u0414\\u0430 \\u0438 \\u043D\\u0435 \\u0442\\u043E\\u043B\\u044C\\u043A\\u043E \\u043A\\u043E\\u0434\\u0438\\u043D\\u0433\\u0430, \\u0430 \\u0441\\u043A\\u043E\\u0440\\u0435\\u0435 \\u0432 \\u043C\\u0438\\u0440 \\u0441\\u043E\\u0437\\u0434\\u0430\\u043D\\u0438\\u044F \\u0438\\u043D\\u0444\\u043E\\u0440\\u043C\\u0430\\u0446\\u0438\\u0438, \\u0438 \\u0432\\u044B \\u043C\\u043E\\u0436\\u0435\\u0442\\u0435 \\u0431\\u044B\\u0442\\u044C \\u043A\\u0435\\u043C \\u0443\\u0433\\u043E\\u0434\\u043D\\u043E, \\u043F\\u0438\\u0441\\u0430\\u0442\\u0435\\u043B\\u0435\\u043C, \\u044E\\u0440\\u0438\\u0441\\u0442\\u043E\\u043C \\u0438\\u043B\\u0438 \\u0436\\u0443\\u0440\\u043D\\u0430\\u043B\\u0438\\u0441\\u0442\\u043E\\u043C. \\u0422\\u0435\\u043A\\u0441\\u0442\\u043E\\u0432\\u044B\\u0435 \\u0440\\u0435\\u0434\\u0430\\u043A\\u0442\\u043E\\u0440\\u044B \\u2014 \\u044D\\u0442\\u043E \\u0447\\u0430\\u0441\\u0442\\u044C \\u043D\\u0430\\u0448\\u0435\\u0439 \\u0436\\u0438\\u0437\\u043D\\u0438. Vim - \\u044D\\u0442\\u043E \\u0442\\u0435\\u043A\\u0441\\u0442\\u043E\\u0432\\u044B\\u0439 \\u0440\\u0435\\u0434\\u0430\\u043A\\u0442\\u043E\\u0440, \\u043A\\u043E\\u0442\\u043E\\u0440\\u044B\\u0439 \\u044F\\u0432\\u043B\\u044F\\u0435\\u0442\\u0441\\u044F \\u043C\\u043E\\u0434\\u0435\\u0440\\u043D\\u0438\\u0437\\u0438\\u0440\\u043E\\u0432\\u0430\\u043D\\u043D\\u043E\\u0439 \\u0432\\u0435\\u0440\\u0441\\u0438\\u0435\\u0439 Vi \\u0432\\u044B\\u043F\\u0443\\u0449\\u0435\\u043D\\u043D\\u043E\\u0433\\u043E \\u0432 1976. \\u042D\\u0442\\u043E \\u0431\\u0443\\u0434\\u0435\\u0442 \\u0441\\u0435\\u0440\\u0438\\u044F \\u0441\\u0442\\u0430\\u0442\\u0435\\u0439 \\u043F\\u043E \\u0438\\u0441\\u043F\\u043E\\u043B\\u044C\\u0437\\u043E\\u0432\\u0430\\u043D\\u0438\\u044E Vim \\u0438 \\u0433\\u043B\\u0430\\u0432\\u043D\\u0430\\u044F \\u0446\\u0435\\u043B\\u044C \\u2014 \\u044D\\u0442\\u043E \\u043F\\u043E\\u043C\\u043E\\u0447\\u044C \\u043F\\u043E\\u043D\\u044F\\u0442\\u044C \\u043E\\u0441\\u043D\\u043E\\u0432\\u043D\\u044B\\u0435 \\u043F\\u0440\\u0438\\u043D\\u0446\\u0438\\u043F\\u044B \\u0440\\u0430\\u0431\\u043E\\u0442\\u044B \\u0441 \\u0440\\u0430\\u0434\\u0430\\u043A\\u0442\\u043E\\u0440\\u043E\\u043C Vim \\u0438 \\u043A\\u0430\\u043A \\u043C\\u043E\\u0436\\u043D\\u043E \\u0441\\u043A\\u043E\\u0440\\u0435\\u0435 \\u043D\\u0430\\u0447\\u0430\\u0442\\u044C \\u0438\\u043C \\u043F\\u043E\\u043B\\u044C\\u0437\\u043E\\u0432\\u0430\\u0442\\u044C\\u0441\\u044F, \\u0432\\u044B\\u0436\\u0438\\u043C\\u0430\\u044F \\u0438\\u0437 \\u043D\\u0435\\u0433\\u043E \\u043C\\u0430\\u043A\\u0441\\u0438\\u043C\\u0443\\u043C \\u043F\\u0440\\u043E\\u0434\\u0443\\u043A\\u0442\\u0438\\u0432\\u043D\\u043E\\u0441\\u0442\\u0438 \\u0438 \\u044D\\u043A\\u043E\\u043D\\u043E\\u043C\\u044F \\u0441\\u0432\\u043E\\u0435 \\u0432\\u0440\\u0435\\u043C\\u044F. \\u0415\\u0441\\u043B\\u0438 \\u0432\\u044B \\u0433\\u043E\\u0442\\u043E\\u0432\\u044B \\u0438\\u043D\\u0432\\u0435\\u0441\\u0442\\u0438\\u0440\\u043E\\u0432\\u0430\\u0442\\u044C \\u0441\\u0432\\u043E\\u0451 \\u0432\\u0440\\u0435\\u043C\\u044F \\u0432 \\u043E\\u0431\\u0443\\u0447\\u0435\\u043D\\u0438\\u0435 \\u0438\\u0441\\u043F\\u043E\\u043B\\u044C\\u0437\\u043E\\u0432\\u0430\\u043D\\u0438\\u044E \\u0442\\u0435\\u043A\\u0441\\u0442\\u043E\\u0432\\u043E\\u0433\\u043E \\u0440\\u0435\\u0434\\u0430\\u043A\\u0442\\u043E\\u0440\\u0430 (\\u0445\\u043E\\u0442\\u044F \\u0441\\u043E \\u0441\\u0442\\u043E\\u0440\\u043E\\u043D\\u044B \\u0437\\u0432\\u0443\\u0447\\u0438\\u0442 \\u043D\\u0435 \\u043C\\u043D\\u043E\\u0433\\u043E \\u0441\\u0442\\u0440\\u0430\\u043D\\u043D\\u043E :), \\u0442\\u043E\\u0433\\u0434\\u0430 ... \\u0414\\u043E\\u0431\\u0440\\u043E \\u043F\\u043E\\u0436\\u0430\\u043B\\u043E\\u0432\\u0430\\u0442\\u044C, \\u0432 \\u0443\\u0434\\u0438\\u0432\\u0438\\u0442\\u0435\\u043B\\u044C\\u043D\\u043E\\u0435 \\u043F\\u0443\\u0442\\u0435\\u0448\\u0435\\u0441\\u0442\\u0432\\u0438\\u0435! \\u042F \\u0431\\u0443\\u0434\\u0443 \\u0438\\u0441\\u043F\\u043E\\u043B\\u044C\\u0437\\u043E\\u0432\\u0430\\u0442\\u044C Vim \\u043D\\u0430 Ubuntu, \\u043D\\u043E \\u0434\\u043B\\u044F \\u0432\\u0441\\u0435\\u0445 \\u0434\\u0440\\u0443\\u0433\\u0438\\u0445 \\u0434\\u0438\\u0441\\u0442\\u0440\\u0438\\u0431\\u0443\\u0442\\u0438\\u0432\\u043E\\u0432 Linux \\u0438 MacOS \\u0432\\u0441\\u0435 \\u043F\\u0440\\u0430\\u043A\\u0442\\u0438\\u0447\\u0435\\u0441\\u043A\\u0438 \\u0438\\u0434\\u0435\\u043D\\u0442\\u0438\\u0447\\u043D\\u043E. \\u0423\\u0441\\u0442\\u0430\\u043D\\u043E\\u0432\\u043A\\u0430 Vim.\")));\n};\n\n_c = AboutMe;\nexport default AboutMe;\n\nvar _c;\n\n$RefreshReg$(_c, \"AboutMe\");","map":{"version":3,"sources":["/home/dima/Desktop/learning/dj-blog/caasy-nextjs-blog-template/components/about-me/AboutMe.js"],"names":["React","Main","styling","AboutMe","header","elements","wrapper"],"mappings":";;;;AAAA,OAAOA,KAAP,MAAkB,OAAlB;AAEA,OAAOC,IAAP,MAAiB,qBAAjB;AAEA,OAAOC,OAAP,MAAoB,uBAApB;AAEA;;;;;;;;AAOA,IAAMC,OAAO,GAAG,SAAVA,OAAU,GAAM;AAElB,SACI;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACI;AAAK,IAAA,SAAS,EAAED,OAAO,CAACE,MAAxB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,IADJ,EAKI;AAAS,IAAA,SAAS,EAAEF,OAAO,CAACG,QAA5B;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACI;AAAK,IAAA,SAAS,EAAEH,OAAO,CAACI,OAAxB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,inJADJ,CALJ,CADJ;AAqBH,CAvBD;;KAAMH,O;AAyBN,eAAeA,OAAf","sourcesContent":["import React from 'react';\n\nimport Main from '../layout/main/Main';\n\nimport styling from './AboutMe.module.scss';\n\n/**\n * Renders a blog post. It dynamically determines the all the elements\n * and renders their corresponding components.\n * @param post {object} data of the blog post\n * @returns {*}\n * @constructor\n */\nconst AboutMe = () => {\n\n    return (\n        <section>\n            <div className={styling.header}>\n\n            </div>\n\n            <article className={styling.elements}>\n                <div className={styling.wrapper}>\n                    Всем привет! Сейчас я вам раскажу про один из наиболее удобных текстовых редакторов под названием Vim. С ним вы откройте себе дорогу в безграничный мир кодинга. Да и не только кодинга, а скорее в мир создания информации, и вы можете быть кем угодно, писателем, юристом или журналистом. Текстовые редакторы — это часть нашей жизни.\n\n                    Vim - это текстовый редактор, который является модернизированной версией Vi выпущенного в 1976. Это будет серия статей по использованию Vim и главная цель — это помочь понять основные принципы работы с радактором Vim и как можно скорее начать им пользоваться, выжимая из него максимум продуктивности и экономя свое время.\n\n                    Если вы готовы инвестировать своё время в обучение использованию текстового редактора (хотя со стороны звучит не много странно :), тогда ...\n\n                    Добро пожаловать, в удивительное путешествие!\n\n                    Я буду использовать Vim на Ubuntu, но для всех других дистрибутивов Linux и MacOS все практически идентично. Установка Vim.\n                </div>\n            </article>\n        </section>\n    );\n};\n\nexport default AboutMe;\n"]},"metadata":{},"sourceType":"module"}